{% if cover_image %}
<style type="text/css">
    #banner { background-image:url('{{ cover_image.cover_small }}'); }
    @media all and (min-width:600px) { #banner { background-image:url('{{ cover_image.cover_medium }}'); } }
    @media all and (min-width:990px) { #banner { background-image:url('{{ cover_image.cover_large  }}'); } }
</style>
{% endif %}
<div id="banner"></div>
<div id="breadcrumb">
    {% if page.breadcrumb or contactInfo %}
    <div class="container">
        {{ page.breadcrumb }}
        {% include '@cob/partials/contactInfo.html.twig' %}

      <div class="social-sharing">
        <div class="item facebook"><svg width="1792" height="1792" viewBox="0 0 1792 1792" xmlns="http://www.w3.org/2000/svg"><path d="M1343 12v264h-157q-86 0-116 36t-30 108v189h293l-39 296h-254v759h-306v-759h-255v-296h255v-218q0-186 104-288.5t277-102.5q147 0 228 12z" fill="#4a4a4a"/></svg></div>
        <div class="item"></div>
        <div class="item"></div>
      </div>
    </div>
    {% endif %}
</div>
